a = int(input("Enter value of a: "))
b = int(input("Enter value of b: "))

# With temporary variable
x = a
y = b
temp = x
x = y
y = temp
print("\nAfter swapping (with temporary variable):")
print("a =", x)
print("b =", y)

# Without temporary variable
a, b = b, a
print("\nAfter swapping (without temporary variable):")
print("a =", a)
print("b =", b)
